version_desc="Show the appjail version."

version_main()
{
	. "${LIBDIR}/version"

	echo "${APPJAIL_VERSION}"
}

version_help()
{
	version_usage

	echo
	echo "${version_desc}"
}

version_usage()
{
	echo "usage: version"
}
