lib_generate_tempfile()
{
	local _f

	_f=`mktemp -t appjail` || exit ${EX_OSERR}

	echo ${_f}
}

lib_generate_tempdir()
{
	local _d

	_d=`mktemp -d -t appjail` || exit ${EX_OSERR}

	echo ${_d}
}
